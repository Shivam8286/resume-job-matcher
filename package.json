{
  "name": "resume-job-matcher",
  "version": "1.0.0",
  "description": "AI-powered resume and job matching platform",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
